{
    "inbounds": [
        {
            "type": "tun",
            "interface_name": "nekoray-tun",
            "inet4_address": "172.19.0.1/28",
            %IPV6_ADDRESS%
            "mtu": %MTU%,
            "auto_route": true,
            "stack": "%STACK%",
            "endpoint_independent_nat": true,
            "sniff": false
        }
    ],
    "outbounds": [
        {
            "type": "socks",
            "tag": "nekoray-socks",
            "server": "127.0.0.1",
            "server_port": %PORT%
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "rules": [
            {
                "network": "udp",
                "port": [
                    135,
                    137,
                    138,
                    139,
                    5353
                ],
                "outbound": "block"
            }
        ]
    }
}